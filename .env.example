# Global or default API key (optional if using tenant-specific keys)
API_KEY=changeme_global

# Gemini API key
GEMINI_API_KEY=your_gemini_key_here

# Database (use Postgres in real env; sqlite for local quick start)
# Example Postgres: postgresql+asyncpg://user:pass@localhost:5432/ai_agent
DATABASE_URL=sqlite+aiosqlite:///./local.db

# CORS origins (comma separated)
CORS_ORIGINS=*

# Auto create tables on startup (true/false)
AUTO_CREATE_TABLES=true
